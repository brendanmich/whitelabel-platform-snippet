<!doctype HTML>

<html lang="en">
	<head>

		<!-- Google Tag Manager -->
		<% if (process.env.ENV === "production") { %>
			<%- include('../components/google_tag_head')  %>
		<% } %>

		<!-- Site Meta -->
		<title>Home - <%= state.entity.name %></title>
		<meta name="description" content="">
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width" />

		<!-- Open Graph Meta -->
		<meta property="og:title" content="Home - <%= state.entity.name %>"/>
		<meta property="og:description" content="" />
		<meta property="og:type" content="website"/>
		<meta property="og:url" content="<%= canonicalUrl %>" />
		<meta property="og:site_name" content="<%= state.entity.name %>"/>
		<% if (state.isCore) { %>
			<meta property="og:image" content="<%= process.env.CDN_URL %>/public/external/social/social-cover.png" />
		<% } else { %>
			<meta property="og:image" content="<%- state.entity.social_cover %>" />
		<% } %>

		<!-- Twitter Meta -->
		<meta name="twitter:card" content="summary">
		<meta name="twitter:site" content="<%= state.entity.twitter.handle %>">
		<meta name="twitter:title" content="Home - <%= state.entity.name %>">
		<meta name="twitter:description" content="">
		<% if (state.isCore) { %>
			<meta name="twitter:image" content="<%= process.env.CDN_URL %>/public/external/social/social-cover.png">
		<% } else { %>
			<meta name="twitter:image" content="<%- state.entity.social_cover %>">
		<% } %>

		<!-- Mobile Meta -->
		<meta name="apple-mobile-web-app-title" content="<%= state.entity.name %>">
		<meta name="application-name" content="<%= state.entity.name %>">

		<!-- Schema -->
		<%- include('../components/schema_org_head') %>

		<!-- External Fonts -->
		<%- include('../components/fonts') %>

		<!-- Styles -->
		<link rel="stylesheet" type="text/css" href="<%= process.env.CDN_URL %>/public/<%= webpack_asset('home','css') %>">

		<!-- Theme Override -->
		<% if (!state.isCore) { %> <%- include('../components/theme_override')  %> <% } %>

		<!-- Manifest -->
		<link rel="manifest" href="/manifest.json" />

		<!-- Mobile Assets -->
		<% if (state.isCore) { %>
			<link rel="apple-touch-icon" href="<%= process.env.CDN_URL %>/public/external/mobile/mobile-icon-192.png">
		<% } else { %>
			<link rel="apple-touch-icon" href="<%- state.entity.mobile_icon %>">
		<% } %>

		<!-- Favicon -->
		<% if (state.isCore) { %>
			<link rel="shortcut icon" href="<%= process.env.CDN_URL %>/public/img/favicon.ico" type="image/x-icon">
			<link rel="icon" href="<%= process.env.CDN_URL %>/public/img/favicon.ico" type="image/x-icon">
		<% } else { %>
			<link rel="shortcut icon" href="<%- state.entity.favicon %>" type="image/x-icon">
			<link rel="icon" href="<%- state.entity.favicon %>" type="image/x-icon">
		<% } %>
	</head>

	<body class="preload">

		<!-- Google Tag Manager -->
		<% if (process.env.ENV === "production") { %>
			<%- include('../components/google_tag_body')  %>
		<% } %>

		<!-- Header -->
		<%- include('../navigation/header.ejs') %>

		<!-- Content -->
		<div class="home-page"></div>

		<!-- Footer -->
		<% if (state.isCore) { %>
			<%- include('../navigation/footer') %>
		<% } else { %>
			<%- include('../navigation/partner_footer') %>
		<% } %>

		<!-- Scripts -->
		<script src="<%= process.env.CDN_URL %>/public/<%= webpack_asset('home','js') %>"></script>
	</body>
</html>
